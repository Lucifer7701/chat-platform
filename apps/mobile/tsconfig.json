{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "target": "ES2020",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["react", "react-native", "node"]
  },
  "include": ["./src", "./index.tsx"],
  "exclude": ["node_modules"]
}


